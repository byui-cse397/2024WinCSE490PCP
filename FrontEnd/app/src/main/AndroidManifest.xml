<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="byui.app.linkUp">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LinkUp"
        tools:targetApi="31">
        <activity
            android:name=".LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EditAccountActivity"
            android:screenOrientation="unspecified">
        </activity>
        <activity android:name=".DeleteActivity"
            android:screenOrientation="unspecified">
        </activity>
        <activity android:name=".SignupActivity"
            android:screenOrientation="unspecified">
        </activity>
        <activity android:name=".AccountActivity"
            android:screenOrientation="unspecified">
        </activity>
        <activity android:name=".PosthomeActivity"
            android:screenOrientation="unspecified">
        </activity>
        <activity android:name=".createpost"
            android:screenOrientation="unspecified">
        </activity>
    </application>

</manifest>